{
  "name": "freechessclub",
  "version": "3.6.1",
  "license": "MIT",
  "description": "Free Chess Club: A modern web client for FICS.",
  "main": "src/app.js",
  "scripts": {
    "app": "tsc src/app.ts && node src/copy-app-files.js && electron src/app.js",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "bundle": "webpack --mode production",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "node src/copy-app-files.js && electron-builder -mwl --x64 --arm64 -p always",
    "release": "yarn build",
    "postinstall": "electron-builder install-app-deps",
    "android": "node src/copy-app-files.js && npx cap run android",
    "ios": "node src/copy-app-files.js && npx cap run ios",
    "start": "webpack-dev-server --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freechessclub/freechessclub-app"
  },
  "author": {
    "name": "Free Chess Club Author(s)",
    "email": "support@freechess.club"
  },
  "build": {
    "appId": "club.freechess.FreeChessClub",
    "productName": "Free Chess Club",
    "copyright": "Copyright Â© 2024 Free Chess Club Author(s)",
    "publish": [
      "github"
    ],
    "files": [
      "play.html",
      "service-worker.js",
      "manifest.json",
      "src/app.js",
      "src/preload.js",
      "assets/**",
      "node_modules/**"
    ],
    "mac": {
      "category": "public.app-category.board-games",
      "identity": "Apple Development: tessellate.ai@gmail.com (W893HC69YA)"
    },
    "linux": {
      "target": [
        "tar.gz",
        "zip"
      ],
      "category": "Game"
    }
  },
  "dependencies": {
    "@capacitor-community/safe-area": "^7.0.0-alpha.1",
    "@capacitor/android": "^7.2.0",
    "@capacitor/core": "^7.2.0",
    "@capacitor/ios": "^7.2.0",
    "@capacitor/preferences": "^7.0.1",
    "@mliebelt/pgn-parser": "^1.4.15",
    "@popperjs/core": "^2.11.6",
    "@uriopass/nosleep.js": "^0.12.2",
    "autolink-js": "freechessclub/autolink-js",
    "bootstrap": "5.2.3",
    "builder-util-runtime": "^9.2.10",
    "capacitor-secure-storage-plugin": "^0.11.0",
    "chess.js": "^0.12.1",
    "chessground": "^9.0.4",
    "cm-polyglot": "^1.1.0",
    "d3": "^7.8.0",
    "electron-updater": "6.6.4",
    "emoji-mart": "^5.6.0",
    "emoji-picker-element": "^1.26.3",
    "jquery": "^3.7.0",
    "js-cookie": "^3.0.1",
    "luxon": "^3.6.1",
    "picmo": "^5.8.5",
    "stockfish.js": "^10.0.2",
    "virtual-scroller": "^1.13.1"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.2.0",
    "@mliebelt/pgn-types": "^1.0.4",
    "@types/bootstrap": "5.2.3",
    "@types/jquery": "^3.5.16",
    "@types/websocket": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "cheerio": "^1.0.0",
    "css-loader": "^6.6.0",
    "electron": "^36.3.2",
    "electron-builder": "26.0.15",
    "eslint": "^8.57.1",
    "eslint-plugin-jsdoc": "^50.6.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "exports-loader": "^3.1.0",
    "file-loader": "^6.2.0",
    "path-browserify": "^1.0.1",
    "style-loader": "^3.3.1",
    "sync-request": "^6.1.0",
    "ts-loader": "^9.3.1",
    "typescript": "^5.8.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^5.0.2",
    "workbox-build": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "packageManager": "yarn@4.9.1"
}
